{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "concurrently \"npm run sass\" \"npm run dev:css\" \"remix dev\" ",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "sass": "sass --watch app/:app/",
    "build:css": "node-sass ./styles -o ./app/styles --output-style compressed",
    "dev:css": "npm run build:css && node-sass -rw ./styles -o ./app/styles",
    "postinstall": "husky install && npx husky set .husky/pre-commit 'npx pretty-quick --staged'",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --quiet",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@remix-run/css-bundle": "^1.17.1",
    "@remix-run/node": "^1.17.1",
    "@remix-run/react": "^1.17.1",
    "@remix-run/vercel": "^1.17.1",
    "@vercel/node": "^2.10.3",
    "isbot": "^3.6.8",
    "node-sass": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0",
    "react-redux": "^8.1.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.17.1",
    "@remix-run/eslint-config": "^1.17.1",
    "@remix-run/serve": "^1.17.1",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "concurrently": "^8.2.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "npkill": "^0.11.3",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "sass": "^1.63.4",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
